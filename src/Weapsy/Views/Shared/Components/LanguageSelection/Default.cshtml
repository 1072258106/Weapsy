@model IEnumerable<Weapsy.Reporting.Languages.LanguageViewModel>

@if (Model.Count() > 1)
{
    <ul class="nav navbar-nav">
        @foreach (var language in Model)
        {
            <li>
                <a href="/home/setlanguage?culture=@language.Url&returnUrl=@Context.Request.Path" title="@language.Name">@language.Name</a>
            </li>
        }
    </ul>
}