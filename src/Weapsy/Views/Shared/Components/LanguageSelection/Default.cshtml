@model IEnumerable<Weapsy.Reporting.Languages.LanguageViewModel>

@if (Model.Count() > 1)
{
    <ul class="nav navbar-nav">
        @foreach (var language in Model)
        {
            <li>
                <a href="@language.Url" title="@language.Name">@language.Name</a>
            </li>
        }
    </ul>
}